# PostgreSQL Version
# Supported: 9.6 (baseline), 12, 15, 16
POSTGRES_VERSION=9.6

# PHP Version
# Supported: 5.6.40 (baseline), 7.4.33, 8.2.x
PHP_VERSION=5.6.40

# PostgreSQL database name for testing
POSTGRES_DB=pl_regression

# PostgreSQL user
POSTGRES_USER=postgres

# PostgreSQL password (default: postgres)
POSTGRES_PASSWORD=postgres
